<div class="container">
  <div class="row" id="articles">

    {{#each article}}

    <article class="selection">
      <h5 class="articleHead">{{title}}</h5>
      <h5 class="pubDate"> Publication Date: {{date}}</h5>
      <h5>Section: {{section}}</h5>
      <a href="{{link}}" class="website">{{link}}</a>
    </article>

    {{/each}}

  </div>
</div>